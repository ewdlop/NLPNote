{
  "app": {
    "bundleName": "com.nlpnote.harmonyos",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    },
    "minAPIVersion": 9,
    "targetAPIVersion": 10,
    "apiReleaseType": "Release"
  },
  "deviceConfig": {
    "default": {
      "network": {
        "usesCleartext": true
      }
    }
  },
  "module": {
    "package": "com.nlpnote.harmonyos",
    "name": ".MainAbility",
    "mainAbility": ".MainAbility",
    "deviceType": [
      "phone",
      "tablet",
      "tv",
      "wearable",
      "car"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "nlpnote",
      "moduleType": "entry"
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": ["entity.system.home"],
            "actions": ["action.system.home"]
          }
        ],
        "orientation": "unspecified",
        "visible": true,
        "name": ".MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard"
      }
    ],
    "js": [
      {
        "pages": ["pages/index/index"],
        "name": ".MainAbility",
        "window": {
          "designWidth": 720,
          "autoDesignWidth": false
        }
      }
    ]
  }
}